{
  "name": "micado_backend",
  "version": "1.0.0",
  "description": "code",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc --copy-resources",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t micado_backend .",
    "docker:run": "docker run -p 3000:3000 -d micado_backend",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^10.1.5",
    "@loopback/authentication-jwt": "^0.14.5",
    "@loopback/boot": "^6.1.5",
    "@loopback/context": "^6.1.5",
    "@loopback/core": "^5.1.5",
    "@loopback/openapi-v3": "^9.1.5",
    "@loopback/repository": "^6.1.5",
    "@loopback/rest": "^13.1.5",
    "@loopback/rest-crud": "^0.17.5",
    "@loopback/rest-explorer": "^6.1.5",
    "@loopback/service-proxy": "^6.1.5",
    "@types/multer": "^1.4.7",
    "ajv": "^8.10.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "https": "^1.0.0",
    "jsdom": "^16.7.0",
    "json2csv": "^5.0.6",
    "jwt-decode": "^3.1.2",
    "keycloak-backend": "^5.0.0",
    "loopback-connector-postgresql": "^7.1.0",
    "loopback-connector-rest": "^5.0.0",
    "loopback-connector-soap": "^8.0.3",
    "multer": "^1.4.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.11",
    "sentence-engine": "^0.8.0",
    "showdown": "^2.0.4",
    "simple-git": "^3.22.0",
    "soap": "^1.1.1",
    "tslib": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.5",
    "@loopback/eslint-config": "^14.0.5",
    "@loopback/testlab": "^6.1.5",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.18.70",
    "@types/node-cron": "^2.0.5",
    "@types/nodemailer": "^6.4.14",
    "@types/showdown": "^1.9.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-mocha": "^10.2.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
